<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <Title>RaiiSharp.Annotations</Title>
    <Description>Annotations for use with the RaiiSharp analyser </Description>
    <Copyright>Copyright Â© John Kelly</Copyright>
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <FileVersion>1.0.0</FileVersion>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
    <DocumentationFile>bin\Debug\RaiiSharp.Annotations.xml</DocumentationFile>
  </PropertyGroup>

  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DocumentationFile>bin\Release\RaiiSharp.Annotations.xml</DocumentationFile>
  </PropertyGroup>

</Project>
